<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Animations" />
    <style name="Animations.PopDownMenu" parent="@style/Animations" />
    <style name="Animations.PopDownMenu.Center" parent="@style/Animations.PopDownMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_top</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_bottom</item>
    </style>
    <style name="Animations.PopDownMenu.Left" parent="@style/Animations.PopDownMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_topleft_to_bottomright</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_bottomright_to_topleft</item>
    </style>
    <style name="Animations.PopDownMenu.Right" parent="@style/Animations.PopDownMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_topright_to_bottomleft</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_bottomleft_to_topright</item>
    </style>
    <style name="Animations.PopDownMenu.Reflect" parent="@style/Animations.PopDownMenu">
        <item name="android:windowEnterAnimation">@anim/pump_top</item>
        <item name="android:windowExitAnimation">@anim/disappear</item>
    </style>
    <style name="Animations.PopUpMenu" parent="@style/Animations" />
    <style name="Animations.PopUpMenu.Center" parent="@style/Animations.PopUpMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_top</item>
    </style>
    <style name="Animations.PopUpMenu.Left" parent="@style/Animations.PopUpMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_bottomleft_to_topright</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_topright_to_bottomleft</item>
    </style>
    <style name="Animations.PopUpMenu.Right" parent="@style/Animations.PopUpMenu">
        <item name="android:windowEnterAnimation">@anim/grow_from_bottomright_to_topleft</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_topleft_to_bottomright</item>
    </style>
    <style name="Animations.PopUpMenu.Reflect" parent="@style/Animations.PopUpMenu">
        <item name="android:windowEnterAnimation">@anim/pump_bottom</item>
        <item name="android:windowExitAnimation">@anim/disappear</item>
    </style>
    <style name="Animations.PopIntoCenter" parent="@style/Animations">
        <item name="android:windowEnterAnimation">@anim/grow_from_center</item>
        <item name="android:windowExitAnimation">@anim/shrink_from_center</item>
    </style>
    <style name="Info.UrlText" parent="@*android:style/TextAppearance.Small">
        <item name="android:textColor">#ff2266ff</item>
    </style>
    <style name="Info.TitleText" parent="@*android:style/TextAppearance.Large">
        <item name="android:textSize">26.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="Info.BylineText" parent="@*android:style/TextAppearance.Medium">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="Dictionary" parent="@*android:style/TextAppearance.Small">
        <item name="android:textColor">#ff111111</item>
    </style>
    <style name="Dictionary.Source" parent="@style/Dictionary">
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Dictionary.Source.NotBest" parent="@style/Dictionary.Source">
        <item name="android:textColor">#ffaaaaaa</item>
    </style>
    <style name="Dictionary.Translation" parent="@style/Dictionary" />
    <style name="Hint.AlignText" parent="@*android:style/TextAppearance.Medium">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="LangHeader.Button" parent="@*android:style/TextAppearance.Medium">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:background">@drawable/button_rounded_grey</item>
    </style>
    <style name="HideTranslationHelpText" parent="@*android:style/TextAppearance.Small">
        <item name="android:textColor">#ff000000</item>
        <item name="android:background">@drawable/hide_trans</item>
    </style>
    <style name="WordLens.Theme" parent="@*android:style/Theme" />
    <style name="Tutorial" />
    <style name="Tutorial.StartButton" parent="@style/Tutorial">
        <item name="android:textColor">@*android:color/white</item>
        <item name="android:background">@drawable/ice_button_xml</item>
        <item name="android:padding">20.0dip</item>
    </style>
    <style name="Tutorial.Text.Bubble" parent="@*android:style/TextAppearance.Medium">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="Tutorial.Text.Header" parent="@*android:style/TextAppearance.Medium">
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="Tutorial.Text.Body" parent="@*android:style/TextAppearance.Large">
        <item name="android:textColor">@*android:color/white</item>
    </style>
    <style name="Tutorial.Text.Body.DemoExplanation" parent="@style/Tutorial.Text.Body" />
    <style name="Tutorial.Text.Body.Big" parent="@style/Tutorial.Text.Body">
        <item name="android:textSize">30.0sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Tutorial.Text.Body.Stylized" parent="@style/Tutorial.Text.Body">
        <item name="android:textSize">32.0sp</item>
        <item name="android:textStyle">italic</item>
    </style>
    <style name="Demo.Watermark.Text" parent="@*android:style/TextAppearance">
        <item name="android:textSize">24.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/red_medium</item>
    </style>
    <style name="Theme.NoWindowBackground" parent="@*android:style/Theme">
        <item name="android:windowBackground">@null</item>
    </style>
    <style name="Theme.NoBackground" parent="@*android:style/Theme">
        <item name="android:background">@null</item>
    </style>
</resources>
